{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "jest/expect-expect": "off",
        "security/detect-object-injection": "off",
        "no-unused-vars": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "class-methods-use-this": "off",
        "no-shadow": "off"
    }
}
